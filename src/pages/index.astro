---
import Layout from '../layouts/Layout.astro';
import Why from '../sections/Why.astro';
import Included from '../sections/Included.astro';
import How from '../sections/How.astro';
import NavbarMobile from '../components/header/NavbarMobile.astro';
import NavbarDesktop from '../components/header/NavbarDesktop.astro';
import Home from '../sections/Home.astro';
import Everything from '../sections/Everything.astro';
import Wrapper from '../structures/Wrapper.astro';
---

<Layout title="">
  <Wrapper>
    <!-- ? navbar mobile -->
    <NavbarMobile isLanding={true} />

    <!-- ? Header section desktop-->
    <NavbarDesktop isLanding={true} />

    <main class="flex flex-col gap-8 md:gap-10 lg:gap-12">
      <Home />
      <Why />
      <Included />
      <How />
      <Everything />
    </main>
  </Wrapper>
</Layout>
